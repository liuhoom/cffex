// }

model ccpm {
  id Int @id @default(autoincrement())
  tradingday DateTime?
  productid String?
  instrumentid String?
  shortname String?

  datatypeid Int?
  varvolume Int? @default(0)
  volume BigInt? @default(0)
}

model rtj {
  id Int @id @default(autoincrement())
  tradingday DateTime?
  productid String?
  instrumentid String?
  season4 Int?    // 当月,下月,下季,隔季  0,1,2,3
  openprice Float?
  closeprice Float?
  highestprice Float?
  lowestprice Float?
  openinterest Int? @default(0)
  volume BigInt? @default(0)
}

model ind {
  id Int @id @default(autoincrement())
  tradingday DateTime?
  instrumentid String?

  volume BigInt?
  open Float?
  high Float?
  low Float?
  close Float?
  chg Float?
}
